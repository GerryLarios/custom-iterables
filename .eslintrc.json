{
  "env": {
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "@tango-io/tango-node",
      "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "camelcase": "off",
      "complexity": "off",
      "lines-between-class-members": "off",
      "no-console": "off",
      "no-magic-numbers": "off",
      "no-shadow": "off"
    }
}
